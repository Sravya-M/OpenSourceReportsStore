{
  "name": "open_source_reports_store",
  "version": "1.0.0",
  "description": "Open Source Reports Store",
  "main": "server.js",
  "engine": {
    "node": "14.2.0",
    "npm": "6.14.8"
  },
  "scripts": {
    "client-install": "cd client && npm install",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Swati N Murthy",
  "license": "ISC",
  "dependencies": {
    "@bit/joshk.jotils.get-random-color": "^2.1.1",
    "@bit/joshk.react-spinners-css.facebook": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.9",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.2.0",
    "nodemailer": "^6.4.13",
    "randomcolor": "^0.6.2",
    "react-bootstrap": "^1.3.0",
    "react-countdown": "^2.3.0",
    "react-file-viewer": "^1.2.1",
    "react-pdf": "^5.0.0",
    "react-spinners-css": "^1.2.2",
    "react-toast-notifications": "^2.4.0",
    "react-toastify": "^6.0.9",
    "react-toasts": "^3.0.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
